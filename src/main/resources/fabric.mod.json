{
	"schemaVersion": 1,
	"id": "fabric-jarvis",
	"version": "${version}",
	"name": "jarvis",
	"description": "Jarvis is a Minecraft mod that leverages advanced AI-powered by a large language model to bring a whole new level of interactivity to your gameplay. Just type /nl <natural language> followed by your command and watch as Jarvis interprets your words and takes action. ",
	"authors": [
		"promising"
	],
	"contact": {
		"sources": "https://github.com/cmypromising/Minecraft-Fabric-Jarvis.git"
	},
	"license": "CC0-1.0",
	"icon": "assets/jarvis/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.promising.jarvis.Jarvis"
		],
		"fabric-datagen": [
			"com.promising.jarvis.JarvisDataGenerator"
		],
		"client": [
			"com.promising.jarvis.JarvisDataClient"
		],
		"modmenu": [
			"com.promising.jarvis.gui.ModMenuIntegration"
		]
	},
	"mixins": [
		"jarvis.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}